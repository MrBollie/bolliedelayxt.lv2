@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix time: <http://lv2plug.in/ns/ext/time#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://ca9.eu/bollie#me>
    a foaf:Person ;
    foaf:name "Bollie" ;
    foaf:mbox <mailto:bollie@ca9.eu> ;
    foaf:homepage <https://ca9.eu/lv2> .

<https://ca9.eu/lv2/bolliedelayxt>
    a lv2:Plugin, lv2:DelayPlugin, doap:Project;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    doap:maintainer <http://ca9.eu/bollie#me> ;
    lv2:microVersion 6 ; lv2:minorVersion 2 ;
    doap:name "Bollie Delay";
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "IP_INPUT_CH1" ;
        lv2:name "In Ch. 1"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "IP_INPUT_CH2" ;
        lv2:name "In Ch. 2"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 2 ;
        lv2:symbol "OP_OUTPUT_CH1" ;
        lv2:name "Out Ch. 1"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 3 ;
        lv2:symbol "OP_OUTPUT_CH2" ;
        lv2:name "Out Ch. 2"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "CP_ENABLED" ;
        lv2:name "Enabled" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "CP_TRAILS" ;
        lv2:name "Trails" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "CP_TEMPO_MODE" ;
        lv2:name "Tempo Mode" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:enumeration, lv2:integer;
        lv2:scalePoint [
            rdf:value 0 ;
            rdfs:label "MOD/Host" ;
            rdfs:comment "Tempo set by MOD/Host" ;
        ], [
            rdf:value 1 ;
            rdfs:label "User" ;
            rdfs:comment "Tempo set by user" ;
        ];
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "CP_TEMPO_HOST" ;
        lv2:name "Host/MOD-Tempo";
        lv2:default 120 ;
        lv2:minimum 6 ;
        lv2:maximum 1000 ;
        lv2:portProperty mod:tapTempo ; 
        lv2:designation  time:beatsPerMinute ;
        units:unit units:bpm ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "CP_TEMPO_USER" ;
        lv2:name "User-Tempo";
        lv2:default 120 ;
        lv2:minimum 6 ;
        lv2:maximum 1000 ;
        units:unit units:bpm ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "CP_TEMPO_DIV_CH1" ;
        lv2:name "Div. Ch. 1" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 5 ;
        lv2:portProperty lv2:enumeration, lv2:integer ;
        lv2:scalePoint [
            rdf:value 0 ;
            rdfs:label "1/4" ;
            rdfs:comment "Simple quarter notes." ;
        ], [
            rdf:value 1 ;
            rdfs:label "1/4T" ;
            rdfs:comment "Triplet quarter notes." ;
        ], [
            rdf:value 2 ;
            rdfs:label "1/8" ;
            rdfs:comment "Simple eighth notes." ;
        ], [
            rdf:value 3 ;
            rdfs:label "1/8." ;
            rdfs:comment "Dotted eighth notes." ;
        ], [
            rdf:value 4 ;
            rdfs:label "1/8T" ;
            rdfs:comment "Triplet eighth notes." ;
        ], [
            rdf:value 5 ;
            rdfs:label "1/16" ;
            rdfs:comment "Sixteenth notes." ;
        ];
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "CP_TEMPO_DIV_CH2" ;
        lv2:name "Div. Ch. 2" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 5 ;
        lv2:portProperty lv2:enumeration, lv2:integer;
        lv2:scalePoint [
            rdf:value 0 ;
            rdfs:label "1/4" ;
            rdfs:comment "Simple quarter notes." ;
        ], [
            rdf:value 1 ;
            rdfs:label "1/4T" ;
            rdfs:comment "Triplet quarter notes." ;
        ], [
            rdf:value 2 ;
            rdfs:label "1/8" ;
            rdfs:comment "Simple eighth notes." ;
        ], [
            rdf:value 3 ;
            rdfs:label "1/8." ;
            rdfs:comment "Dotted eighth notes." ;
        ], [
            rdf:value 4 ;
            rdfs:label "1/8T" ;
            rdfs:comment "Triplet eighth notes." ;
        ], [
            rdf:value 5 ;
            rdfs:label "1/16" ;
            rdfs:comment "Sixteenth notes." ;
        ];
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "CP_FB" ;
        lv2:name "Feedback" ;
        lv2:default -12.0 ;
        lv2:minimum -97.0 ;
        lv2:maximum -1.0 ;
        units:unit units:db ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "CP_CF" ;
        lv2:name "Crossfeed" ;
        lv2:default -12.0 ;
        lv2:minimum -97.0 ;
        lv2:maximum -1.0 ;
        units:unit units:db ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "CP_GAIN_IN" ;
        lv2:name "Input Gain" ;
        lv2:default 0.0 ;
        lv2:minimum -97.0 ;
        lv2:maximum 12.0 ;
        units:unit units:db ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "CP_GAIN_DRY" ;
        lv2:name "Dry Gain" ;
        lv2:default 0.0 ;
        lv2:minimum -97.0 ;
        lv2:maximum 12.0 ;
        units:unit units:db ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "CP_GAIN_WET" ;
        lv2:name "Wet Gain" ;
        lv2:default -12.0 ;
        lv2:minimum -97.0 ;
        lv2:maximum 12.0 ;
        units:unit units:db ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 16 ;
        lv2:symbol "CP_MOD_ON" ;
        lv2:name "Modulation on" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 17 ;
        lv2:symbol "CP_MOD_DEPTH" ;
        lv2:name "Mod. Depth" ;
        lv2:default 2.0 ;
        lv2:minimum 0.1 ;
        lv2:maximum 5.0 ;
        units:unit units:ms ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 18 ;
        lv2:symbol "CP_MOD_RATE" ;
        lv2:name "Mod. Rate" ;
        lv2:default 0.1 ;
        lv2:minimum 0.1 ;
        lv2:maximum 3.0 ;
        lv2:portProperty pprop:logarithmic ;
        units:unit units:hz ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 19 ;
        lv2:symbol "CP_HCF_PRE_ON" ;
        lv2:name "High Cut Pre On" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 20 ;
        lv2:symbol "CP_HCF_PRE_FREQ" ;
        lv2:name "High Cut Pre Freq." ;
        lv2:default 7500.0 ;
        lv2:minimum 200.0 ;
        lv2:maximum 22000.0 ;
        lv2:portProperty pprop:logarithmic ;
        units:unit units:hz ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 21 ;
        lv2:symbol "CP_HCF_PRE_Q" ;
        lv2:name "High Cut Pre Q." ;
        lv2:default 1.0 ;
        lv2:minimum 0.125 ;
        lv2:maximum 8.0 ;
        lv2:portProperty pprop:logarithmic ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 22 ;
        lv2:symbol "CP_LCF_PRE_ON" ;
        lv2:name "Low Cut Pre On" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 23 ;
        lv2:symbol "CP_LCF_PRE_FREQ" ;
        lv2:name "Low Cut Pre Freq." ;
        lv2:default 20.000 ;
        lv2:minimum 20.000 ;
        lv2:maximum 2000.000 ;
        lv2:portProperty pprop:logarithmic ;
        units:unit units:hz ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 24 ;
        lv2:symbol "CP_LCF_PRE_Q" ;
        lv2:name "Low Cut Pre Q." ;
        lv2:default 1.0 ;
        lv2:minimum 0.125 ;
        lv2:maximum 8.0 ;
        lv2:portProperty pprop:logarithmic ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 25 ;
        lv2:symbol "CP_HCF_FB_ON" ;
        lv2:name "High Cut Feedback On" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 26 ;
        lv2:symbol "CP_HCF_FB_FREQ" ;
        lv2:name "High Cut Feedback Freq." ;
        lv2:default 7500.0 ;
        lv2:minimum 200.0 ;
        lv2:maximum 22000.0 ;
        lv2:portProperty pprop:logarithmic ;
        units:unit units:hz ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 27 ;
        lv2:symbol "CP_HCF_FB_Q" ;
        lv2:name "High Cut Feedback Q." ;
        lv2:default 1.0 ;
        lv2:minimum 0.125 ;
        lv2:maximum 8.0 ;
        lv2:portProperty pprop:logarithmic ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 28 ;
        lv2:symbol "CP_LCF_FB_ON" ;
        lv2:name "Low Cut Feedback On" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 29 ;
        lv2:symbol "CP_LCF_FB_FREQ" ;
        lv2:name "Low Cut Feedback Freq." ;
        lv2:default 20.000 ;
        lv2:minimum 20.000 ;
        lv2:maximum 2000.000 ;
        lv2:portProperty pprop:logarithmic ;
        units:unit units:hz ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 30 ;
        lv2:symbol "CP_LCF_FB_Q" ;
        lv2:name "Low Cut Feedback Q." ;
        lv2:default 1.0 ;
        lv2:minimum 0.125 ;
        lv2:maximum 8.0 ;
        lv2:portProperty pprop:logarithmic ;
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 31 ;
        lv2:symbol "tempo_out" ;
        lv2:name "Current tempo" ;
        lv2:default 120 ;
        lv2:minimum 6 ;
        lv2:maximum 1000 ;
        units:unit units:bpm ;
    ] ;
    rdfs:comment '''This stereo tempo delay features high pass and low pass filters as well as host tempo. When using it with the MOD Duo on software version >1.2.0, then please assign a footswitch to Host/MOD-Tempo. Otherwise you can assign the tap button to a foot switch. Always make sure to set the correct tempo mode. 
    Enjoy! :-) And feedback is always welcome.''' .
